    .getRecipes()\n      .then((recipesList) => setRecipes(recipesList.recipes.data));\n  }, []);\n  if (loggedIn) {\n    if (isAdmin) {\n      return (\n        <div>\n          <AdminPanel\n            firstName={firstName}\n            recipes={recipes}\n            currentUser={currentUser}\n          />\n        </div>\n      );\n    } else {\n      return (\n        <div>\n          <UserPanel\n            firstName={firstName}\n            recipes={recipes}\n            products={products}\n            stripePromise={stripePromise}\n            currentUser={currentUser}\n          />\n        </div>\n      );\n    }\n  } else {\n    return (\n      <div>\n        <Switch>\n          <Route exact path=\"/\" render={() => <HomePage recipes={recipes} />} />\n          <Route\n            path=\"/shop/:id\"\n            render={(routerProps) => {\n              const itemId = routerProps.match.params.id;\n              const item = products.find((product) => {\n                return product.id.toString() === itemId;\n              });\n              return item ? <SingleProductPage item={item} /> : <Spinner />;\n            }}\n          />\n          <Route\n            path=\"/recipes/:id\"\n            render={(routerProps) => {\n              const recipeId = routerProps.match.params.id;\n              const recipe = recipes.find((recipe) => {\n                return recipe.id.toString() === recipeId;\n              });\n              return recipe ? <SingleRecipe recipe={recipe} /> : <Spinner />;\n            }}\n          />\n\n          <Route\n            path=\"/shop\"\n            render={() => <ShopPage products={products} recipes={recipes} />}\n          />\n          <Route\n            path=\"/checkout\"\n            render={() => (\n              <Elements stripe={stripePromise}>\n                <CheckoutPage />\n              </Elements>\n            )}\n          />\n          <Route path=\"/recipes\" render={() => <RecipesPage />} />\n        