const response = await fetch(`${API_ROOT}/recipes/new`, config);\n  return response.json();\n};\n\nconst editRecipe = async (id, config) => {\n  const response = await fetch(`${API_ROOT}/recipes/${id}/edit`, config);\n  return response.json();\n};\nconst deleteRecipe = async (id, config) => {\n  const response = await fetch(`${API_ROOT}/recipes/${id}/delete`, config);\n  return response.json();\n};\n\nconst autoLogin = () => {\n  return fetch(`${API_ROOT}/autologin`, {\n    credentials: \"include\",\n  }).then((res) => res.json());\n};\n\nconst passwordReset = async (config) => {\n  const response = await fetch(`${API_ROOT}/forgot_password`, config);\n  return response.json();\n};\n\nconst logOut = (config) => {\n  return fetch(`${API_ROOT}/signout`, config);\n};\n\nconst getCS = (config) => {\n  return fetch(`${API_ROOT}/secret`, config);\n};\n\nexport default {\n  API_ROOT: API_ROOT,\n  auth: {\n    login: login,\n    autoLogin: autoLogin,\n    logOut: logOut,\n    passwordReset: passwordReset,\n  },\n  user: {\n    signUp: signUp,\n  },\n  checkout: {\n    getCS: getCS,\n  },\n  product: {\n    getProducts: getProducts,\n  },\n  youtube: {\n    recipesPlaylist: recipesPlaylist,\n    getVideos: getVideos,\n  },\n  recipes: {\n    getRecipes: getRecipes,\n    newRecipe: newRecipe,\n    deleteRecipe: deleteRecipe,\n    editRecipe: editRecipe,\n  },\n};\n","/Users/Sdever/Development/code/mod-5/can-gwen-cook-frontend/src/components/footer/Footer.js",[],"/Users/Sdever/Development/code/mod-5/can-gwen-cook-frontend/src/containers/main-container/MainContainer.js",["265"],"import React, { useState, useEffect } from \"react\";\nimport { useSelector } from \"react-redux\";\nimport { Route, Switch } from \"react-router-dom\";\nimport HomePage from \"../../pages/homepage/Homepage\";\nimport RecipesPage from \"../../pages/recipes/Recipes\";\nimport ShopPage from \"../../pages/shop/Shop\";\nimport CheckoutPage from \"../../pages/checkout/Checkout\";\nimport AboutPage from \"../.